{
  "name": "ts-api-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "prepare": "husky install",
    "test:staged": "NODE_ENV=test jest --passWithNoTests --runInBand",
    "test:watch": "npx jest --watchAll --passWithNoTests --runInBand",
    "lint": "eslint .",
    "dev": "npx kill-port 5000 && NODE_ENV=development nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register ./src/server.ts' -e ts",
    "start": "node dist/server.js",
    "dev:migration:run": "npm run dev:load-envs prisma migrate deploy",
    "dev:migration:generate": "npm run dev:load-envs prisma migrate dev",
    "test:migration:run": "npm run test:load-envs prisma migrate deploy",
    "test:migration:generate": "npm run test:load-envs prisma migrate dev",
    "lint:staged": "lint-staged",
    "dev:load-envs": "dotenv -e .env.development",
    "dev:seed": "npm run dev:load-envs prisma db seed",
    "test:seed": "npm run test:load-envs prisma db seed",
    "test:load-envs": "dotenv -e .env.test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaniloLO53/ts-api-template.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DaniloLO53/ts-api-template/issues"
  },
  "homepage": "https://github.com/DaniloLO53/ts-api-template#readme",
  "prisma": {
    "seed": "npm run dev:load-envs -- ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@prisma/client": "^4.12.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.37.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-newlines": "^1.3.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "lint-staged": "^13.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "prisma": "^4.12.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "typescript-transform-paths": "^3.4.6"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "googleapis": "^118.0.0",
    "http-status": "^1.6.2",
    "joi": "^17.9.1",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.3.0",
    "mongoose": "^7.0.4",
    "passport": "^0.6.0",
    "passport-google-oauth2": "^0.2.0",
    "reflect-metadata": "^0.1.13",
    "zod": "^3.21.4"
  }
}
